{
  "buildPlugins": [
    {
      "name": "@flash-install/vercel-plugin",
      "config": {
        "enableCache": true,
        "cacheCompression": true,
        "concurrency": 4,
        "fallbackToNpm": true
      }
    }
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
